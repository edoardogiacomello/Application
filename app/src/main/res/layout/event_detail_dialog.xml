<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/RelativeLayout1"
    android:layout_width="fill_parent"
    android:layout_height="wrap_content"
    android:orientation="vertical"
    android:paddingTop="10dp" >
	
    <RelativeLayout 
        android:id="@+id/first"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:orientation="horizontal">
	    <TextView
	        android:id="@+id/text_event_name"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_toLeftOf="@+id/info_event_name"
	        android:layout_toStartOf="@+id/info_event_name"
	        android:layout_alignParentLeft="true"
	        android:layout_alignParentStart="true"
	        android:text="@string/text_event_name" />
	    
	     <TextView
	        android:id="@+id/info_event_name"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentRight="true"
	        android:layout_alignParentEnd="true"
	        android:text="@string/text_none"
	        android:textStyle="bold"
         />
	</RelativeLayout>
	
    <RelativeLayout
        android:id="@+id/second" 
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/first"
        android:padding="10dp"
        android:orientation="horizontal">
	    <TextView
	        android:id="@+id/text_event_start"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_toLeftOf="@+id/info_event_start"
	        android:layout_toStartOf="@+id/info_event_start"
	        android:layout_alignParentLeft="true"
	        android:layout_alignParentStart="true"
	        android:text="@string/text_event_start" />
	    <TextView
	        android:id="@+id/info_event_start"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentRight="true"
	        android:layout_alignParentEnd="true"
	        android:text="@string/text_none"
	        android:textStyle="bold" />
	</RelativeLayout>
	
	<RelativeLayout 
	    android:id="@+id/third"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/second"
        android:padding="10dp"
        android:orientation="horizontal">
	    <TextView
	        android:id="@+id/text_event_end"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_toLeftOf="@+id/info_event_end"
	        android:layout_toStartOf="@+id/info_event_end"
	        android:layout_alignParentLeft="true"
	        android:layout_alignParentStart="true"
	        android:text="@string/text_event_end" />
	    <TextView
	        android:id="@+id/info_event_end"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentRight="true"
	        android:layout_alignParentEnd="true"
	        android:text="@string/text_none"
	        android:textStyle="bold" />
	</RelativeLayout>
	
	<RelativeLayout 
	    android:id="@+id/fourth"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:layout_below="@+id/third"
        android:orientation="horizontal">
	    <TextView
	        android:id="@+id/text_event_location"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_toLeftOf="@+id/info_event_location"
	        android:layout_toStartOf="@+id/info_event_location"
	        android:layout_alignParentLeft="true"
	        android:layout_alignParentStart="true"
	        android:text="@string/text_event_location" />
	    <TextView
	        android:id="@+id/info_event_location"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentRight="true"
	        android:layout_alignParentEnd="true"
	        android:text="@string/text_none"
	        android:textStyle="bold" />
	</RelativeLayout>
	
	<RelativeLayout 
	    android:id="@+id/fifth"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:padding="10dp"
        android:layout_below="@+id/fourth"
        android:orientation="horizontal">
	    <TextView
	        android:id="@+id/text_event_category"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_toLeftOf="@+id/info_event_category"
	        android:layout_toStartOf="@+id/info_event_category"
	        android:layout_alignParentLeft="true"
	        android:layout_alignParentStart="true"
	        android:text="@string/text_event_category" />
	    <TextView
	        android:id="@+id/info_event_category"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentRight="true"
	        android:layout_alignParentEnd="true"
	        android:text="@string/text_none"
	        android:textStyle="bold" />
	</RelativeLayout>
	
	<RelativeLayout 
	    android:id="@+id/sixth"
	        android:layout_width="fill_parent"
	        android:layout_height="wrap_content"
	        android:padding="10dp"
	        android:layout_below="@+id/fifth"
	        android:orientation="horizontal">
	    <TextView
	        android:id="@+id/text_event_movable"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_toLeftOf="@+id/info_event_movable"
	        android:layout_toStartOf="@+id/info_event_movable"
	        android:layout_alignParentLeft="true"
	        android:layout_alignParentStart="true"
	        android:text="@string/text_event_movable" />    
	    <TextView
	        android:id="@+id/info_event_movable"
	        android:layout_width="wrap_content"
	        android:layout_height="wrap_content"
	        android:layout_alignParentRight="true"
	        android:layout_alignParentEnd="true"
	        android:text="@string/text_no"
	        android:textStyle="bold" />
	</RelativeLayout>

</RelativeLayout>
